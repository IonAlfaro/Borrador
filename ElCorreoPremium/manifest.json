{
  "manifest_version": 2,

  "name": "ElcorreoPremium",
  "description": "Bloquea el limite de visita de noticias",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "storage",
    "cookies",
    "webRequest",
    "*://*.elcorreo.com/*"
  ]
}